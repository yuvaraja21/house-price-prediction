{
  "family": "housing-api-task-ecs",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::261899902410:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::440913953032:role/ecsTaskRole",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "containerDefinitions": [
    {
      "name": "housing-api",
      "image": "440913953032.dkr.ecr.eu-north-1.amazonaws.com/housing-api:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/housing-api-task-ecs",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
